 <!-- Check to see which navbar partial to load -->
<div class='container'>
  <% if (session.authenticated) { %>
  <%- partial ('../partials/navbar_loggedin.ejs') %>
  <% } else { %>
  <%- partial ('../partials/navbar_loggedout.ejs') %>
  <% } %>
</div>

<div id="kindness-story-wrapper">
  <div class="story-page-top">
    <h2>Kindness Story</h2>
    <div class="kindness-story-item container">
        <div class="toppish">
        </div>
        <div class="inspiration-text">
          <%= story[0].insp_text %>
        </div>
        <div id="kindness-created">Created by <%= story[0].created_by %>
        </div>
    </div>
  </div>
</div>

<% for (var i in story) { %>
  <% for (var x in story[0].events) { %>
  <div class="story-event">
      This kindness was pinned by: <%= story[0].events[x].event.pinned_by %>
      <br>
      pinned from: <%= story[0].events[x].event.pinned_from %>
  </div>
  <% } %>
<% } %>

