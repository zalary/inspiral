  <div class="col-xs-6">
    <span class="inspiration-column-title">
     <div> <%= story[0].insp_text %> </div>
    </span>

      <% for (var i in story) { %>
         This inspiration was created by <%= story[0].created_by %>
        <% for (var x in story[0].events) { %>
        <div class="story-event">

            This inspiration was pinned by: <%= story[0].events[x].event.pinned_by %> from: <%= story[0].events[x].event.pinned_from %>


          </div>
        <% } %>
      <% } %>
    </div>

</div>
